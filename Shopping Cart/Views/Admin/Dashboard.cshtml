
@{
    ViewBag.Title = "Dashboard";
}

<h2>Dashboard</h2>

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<table class="table table-bordered table-sm">
    <thead>
        <tr>
            <th>Order Id</th>
            <th>Order Date</th>
            <th>Customer Name</th>
            <th>Phone No</th>
            <th>Address</th>
            <th>Total</th>
            <th>Status</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var pro in ViewBag.p)
        {
        <tr>
            <td>@pro.oid</td>
            <td>@pro.odate</td>
            <td>@pro.opname</td>
            <td>@pro.opphone</td>
            <td>@pro.opsaddress</td>
            <td>@pro.oamount</td>
            <td>@pro.ostatus</td>
        </tr>

        }
        
    </tbody>
</table>